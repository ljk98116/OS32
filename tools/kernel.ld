ENTRY(start)
SECTIONS
{
    /* 段起始位置 */

    . = 0x00100000;
    .text :
    {
        . = ALIGN(4);
        KEEP(*(.multiboot))
        . = ALIGN(4096);
        *(.text)
        . = ALIGN(4096);
    }

    .data :
    {
        *(.data)
        *(.rodata)
        . = ALIGN(4096);
    }

    .bss :
    {
        *(.bss)
        . = ALIGN(4096);
    }

    .stab :
    {
        *(.stab)
        . = ALIGN(4096);
    }
    
    .stabstr :
    {
        *(.stabstr)
        . = ALIGN(4096);
    }

    /DISCARD/ : { *(.comment) *(.eh_frame) }
}